{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Dify Client",
  "version": "1.0.0",
  "identifier": "com.dify.client",
  "build": {
    "beforeDevCommand": "vite",
    "beforeBuildCommand": "vite build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "title": "Dify Client",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "resizable": true,
        "decorations": true,
        "transparent": false,
        "maximized": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "Copyright © 2025 Dify Client",
    "category": "DeveloperTool",
    "shortDescription": "Dify工作流客户端",
    "longDescription": "基于Tauri 2.9 + Vue 3构建的Dify工作流客户端，支持文件上传、工作流执行、节点监控等功能。",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    }
  },
  "plugins": {
    "shell": {
      "all": true
    },
    "dialog": {
      "all": true
    },
    "fs": {
      "all": true,
      "scope": ["**"]
    },
    "http": {
      "all": true,
      "scope": ["**"]
    },
    "notification": {
      "all": true
    },
    "window": {
      "all": true
    },
    "store": {
      "all": true
    }
  }
}